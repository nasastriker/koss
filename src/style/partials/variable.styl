white = #fff
hite = #f9f8f8
dark = #262729
eee = #eeeeee
dblue = #838d8f
ddd = #d8ddde


*
	box-sizing border-box
	outline none
	text-decoration: none
a
	&:visited
	&:link
	&:active
		text-decoration none

ul
	list-style: none
	padding 0
	margin 0
	
:focus::-webkit-input-placeholder 
	color transparent
:focus::-moz-placeholder          
	color transparent
:focus:-moz-placeholder           
	color transparent
:focus:-ms-input-placeholder      
	color transparent
